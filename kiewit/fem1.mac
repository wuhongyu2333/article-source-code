secoffset,cent
secdata,180e-3,180e-3,630e-3,22e-3,22e-3,17e-3


!!定义节点
num_nodes=n*(1+m)*m/2+1    		 			!!节点总数
N,0,0,0,H            							!!圆心节点
N,num_nodes+1,0,0,-100            					!!坐标系原点
*do,i,1,m
  *do,j,1,n*i
    inode=(i-1)*i*n/2+1+j        	!!当前节点号
    iradius=d_ring*i          		!!当前环半径
    itheta=2*PI*(j-1)/(n*i)       !!当前节点角度
    iheight=sqrt(arc**2-iradius**2)-arc+H 				!!当前节点矢高
    N,inode,iradius*cos(itheta),iradius*sin(itheta),iheight
  *enddo
*enddo

!!定义单元
*do,i,1,m-1                         !!定义环向杆件
  type,1
  secnum,INPUTDATA(i+2)
  mat,1
  *do,j,1,n*i-1
    inode=(i-1)*i*n/2+1+j           !!节点号
    e,inode,inode+1,num_nodes+1
  *enddo
  e,(i-1)*i*n/2+n*i+1,(i-1)*i*n/2+2,num_nodes+1
*enddo

type,1
secnum,INPUTDATA(7)
mat,1
*do,i,2,n+1		                       !!定义径向杆件
   e,1,i,num_nodes+1
  *do,j,1,m-1
    inode_start=(j-1)*j*n/2+j*(i-2)+2            						!!首节点号
    inode_end=(j+1)*j*n/2+(j+1)*(i-2)+2            					!!尾节点号
    e,inode_start,inode_end,Nnode+1
  *enddo
*enddo

*do,i,1,m-1		                       !!定义支撑杆件
  type,1
  secnum,INPUTDATA(i+7)
  mat,1
  *do,j,1,n
  	*dim,line1,array,1,i+1,1
  	*do,k,1,i+1
  		*if,j,eq,n,and,k,eq,i+1,then
      	line1(1,k)=(i-1)*i*n/2+2
    	*else
      	line1(1,k)=(i-1)*i*n/2+(j-1)*i+k+1
      *endif
  	*enddo
  	
  	*dim,line2,array,1,i,1
  	*do,k,1,i
  		line2(1,k)=(i+1)*i*n/2+(j-1)*(i+1)+k+2
  	*enddo
  	
  	*dim,line,array,1,2*i+1,1
  	*do,k,1,i
  		line(1,2*k-1)=line1(1,k)
  		line(1,2*k)=line2(1,k)
  	*enddo
  	line(1,2*i+1)=line1(1,i+1)
  	
  	*do,b,1,2*i
  		e,line(1,b),line(1,b+1),Nnode+1
  	*enddo
  	
  	line1=
  	line2=
  	line=

  *enddo
*enddo



!!!!施加约束
nsel,s,,,(m-1)*m*n/2+2,(m+1)*m*n/2+1
d,all,ux,0
d,all,uy,0
d,all,uz,0
!d,all,rotx,0
!d,all,roty,0
!d,all,rotz,0


!!!施加荷载
fd=PI*R*R*dead_load/(n*(m-1)*m/2+1)
fl=PI*R*R*live_load/(n*(m-1)*m/2+1)
fw=(ASIN(R/arc)*arc*arc-R*arc+R*R)*wind_load*2/(n*(m-1)*m/2+1)
ACEL,,,9.8
nsel,s,,,1,(m-1)*m*n/2+1
f,all,fz,-fd
nsel,s,loc,x,0,R
f,all,fx,-fw
f,all,fz,-fl
nsel,all

!!!首先进行静力分析
/solu 
antype,0
pstres,on         					!!打开预应力开关
solve 
finish                		 			!!结束当前求解


!!!然后进行屈曲分析
/solu 
antype,1      
bucopt,lanb,1 
mxpand,1,,,1 
solve 
finish


/post1
NSORT,U,SUM,0								
*get,u_max,SORT,0,MAX

*GET,STRESS_MAX,SECR,,S,EQV,MAX
*GET,STRESS_MIN,SECR,,S,EQV,MIN



/prep7
UPGEOM,span/300/abs(u_max),LAST,LAST,'fem1','rst',''
finish


/solu
antype,0
nlgeom,on
arclen,on
arctrm,U,1,,uz													!对弧长法求解进行终止控制
neqit,30														!容许的最大平衡迭代次数
nsubst,30														!设置子步数
outres,all,all
solve
finish


/post26
prvar,4
nsol,2,1,u,z
prod,3,2,,,,,,1000
prod,4,1,,,,,,30
/axlab,x,Uz(mm)
/axlab,y,K
xvar,3
plvar,4
!!获得荷载稳定系数
*GET,NUM_IT,VARI,4,NSETS
*do,i,1,NUM_IT-1
	*GET,k1,VARI,4,RSET,i
	*GET,k2,VARI,4,RSET,i+1
	*IF,K1,gt,K2,then
		STAB_EFF=k1
		*exit
	*else
		STAB_EFF=k2
	*endif
*enddo

finish

!*cfopen,D:\SOFTWARE\ANSYS\dome_2\output,txt,,
*cfopen,output,txt,,
*vwrite,u_max,STRESS_MAX,STAB_EFF
(e15.6,2x,e15.6,2x,f10.5)
*cfclos
