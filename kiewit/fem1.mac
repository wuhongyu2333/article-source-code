!!参数输入
/prep7
!!材料特性
PI=ACOS(-1)												!!pi
Ex=2.06e11												!!弹性模量
Nu=0.3														!!泊松比
Fz=100000          				!!节点荷载
!!几何特性
dead_load=3000						!恒载3kN/m2
live_load=1500						!活载1.5kN/m2
wind_load=1000						!风载1kN/m2


span=80
